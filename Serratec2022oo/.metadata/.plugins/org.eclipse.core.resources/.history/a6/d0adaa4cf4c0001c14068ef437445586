package org.serratec.model;

public class TesteFuncionario {

	public static void main(String[] args) {

		Funcionario[] funcionario = new Funcionario[3];
		funcionario[0] = new Funcionario("Vinicius", "Gerente de TI", 8000);
		funcionario[1] = new Funcionario("Roni", "Professor", 80000);
		funcionario[2] = new Funcionario("Adrian", "Programado", 6000);
		
		for (int i = 0; i < funcionario.length; i++) {
			System.out.println(funcionario[i]);
		}
		
		for (Funcionario funcionario2 : funcionario) {
			System.out.println(funcionario);
			
		}
	}

}
