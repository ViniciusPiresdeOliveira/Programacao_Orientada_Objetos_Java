package org.serratec.enumeradores;

import java.time.LocalDate;

public class Cavalo extends Animal implements AnimalCompeticao {

	private String marcaFerradura;
	
	public Cavalo(String nome, LocalDate dataVacinacao, String marcaFerradura) {
		super(nome, dataVacinacao);
		this.marcaFerradura = marcaFerradura;
	}
	
	@Override
	public String toString() {
		return "Cavalo: " + marcaFerradura + "\nNome: " + nome + "\nData da vacinacao: " + dataVacinacao;
	}

	@Override
	public void trocarFerradura() {

	}

	@Override
	public void viajar() {

	}

	@Override
	public String emitirSom() {
		return null;
	}

}
